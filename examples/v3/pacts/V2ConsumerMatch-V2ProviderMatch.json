          },
          "$.query.baz[1]": {
            "match": "regex",
            "regex": "[a-z]+"
          },
          "$.query.baz[2]": {
            "match": "regex",
            "regex": "[a-z]+"
          }
        },
        "method": "POST",
        "path": "/foobar",
        "query": "baz=bar&baz=bat&baz=baz"
      },
      "response": {
        "status": 200
      }
    }
  ],
  "metadata": {
    "pactRust": {
      "version": "0.9.2"
