        "matchingRules": {
          "$.body.datetime": {
            "match": "type"
          },
          "$.body.id": {
            "match": "type"
          },
          "$.body.lastName": {
            "match": "type"
          },
          "$.body.name": {
            "match": "type"
          },
          "$.header.Authorization": {
            "match": "type"
          },
          "$.header.Content-Type": {
            "match": "regex",
            "regex": "application\\/json"
          },
          "$.path": {
            "match": "regex",
